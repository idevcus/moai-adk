{
  "name": "moai-adk",
  "version": "0.0.3",
  "description": "ðŸ—¿ MoAI-ADK: TypeScript-based SPEC-First TDD Development Kit with Universal Language Support",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "moai": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.2.0"
  },
  "packageManager": "bun@1.2.19",
  "scripts": {
    "build": "tsup && bun run build:scripts",
    "build:hooks": "tsup --config tsup.hooks.config.ts",
    "build:scripts": "tsup --config tsup.scripts.config.ts",
    "build:watch": "tsup --watch",
    "dev": "tsx src/cli/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "check:biome": "biome check src",
    "check:biome:fix": "biome check --write src",
    "type-check": "tsc --noEmit --incremental false",
    "clean": "rimraf dist .turbo node_modules/.cache",
    "check": "bun run type-check && bun run check:biome",
    "ci": "bun run clean && bun run build && bun run check && bun run test:ci",
    "//prepublishOnly": "bun run ci"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "inquirer": "^12.9.6",
    "mime-types": "^2.1.35",
    "mustache": "^4.2.0",
    "semver": "^7.7.2",
    "simple-git": "^3.28.0",
    "yaml": "^2.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/mime-types": "^2.1.4",
    "@types/mustache": "^4.2.5",
    "@types/node": "^24.5.2",
    "@types/semver": "^7.7.1",
    "@vitest/spy": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "moai",
    "adk",
    "cli",
    "development",
    "typescript",
    "tdd"
  ],
  "author": "MoAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/moai-adk.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/moai-adk/issues"
  },
  "homepage": "https://github.com/your-org/moai-adk#readme"
}
