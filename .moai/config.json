{
  "project": {
    "mode": "personal",
    "name": "MoAI-ADK",
    "description": "MoAI Agentic Development Kit"
  },
  "git_strategy": {
    "personal": {
      "auto_checkpoint": true,
      "checkpoint_interval": 300,
      "max_checkpoints": 50,
      "cleanup_days": 7,
      "branch_prefix": "feature/",
      "auto_commit": true
    },
    "team": {
      "use_gitflow": true,
      "main_branch": "main",
      "develop_branch": "develop",
      "feature_prefix": "feature/SPEC-",
      "auto_pr": true,
      "draft_pr": true
    }
  },
  "constitution": {
    "simplicity_threshold": 3,
    "test_coverage_target": 85,
    "enforce_tdd": true,
    "require_tags": true
  }
}